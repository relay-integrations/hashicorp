{
  "$schema": "http://json-schema.org/draft-07/schema",
  "type": "object",
  "properties": {
    "plan": {
      "type": "string",
      "description": "Object containting a base64-encoded Terraform .tfplan",
    },
    "changed": {
      "type": "boolean",
      "description": "Boolean set to true if any resources will be changed by running the plan"
    }
  },
  "required": ["plan"]
}

